package com.example.demo.uss.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.example.demo.uss.repository.AdminRepository;
import com.example.demo.uss.repository.AdminRepositoryImpl;
import com.example.demo.uss.repository.UserRepositoryImpl;

import lombok.RequiredArgsConstructor;

@Service 
public class AdminServiceImpl implements AdminService {
	private final AdminRepositoryImpl repo;
	
	@Autowired
	public AdminServiceImpl(AdminRepositoryImpl repo) {
		this.repo=repo;
	}

}